    "ConvertTimeout": 60
  },
  "Manage": {
    "IpWhitelist": [
      "*"
    ]
  },
  "Cluster": {
    "SelfServerId": 1,
    "Servers": [
      {
        "Id": 1,
        "Host": "127.0.0.1:5001",
        "Weight": 1,
        "AllowUpload": true
      }
    ]
  },
  "Audience": {
