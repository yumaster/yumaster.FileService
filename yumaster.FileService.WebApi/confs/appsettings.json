{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "LogsDir": "/var/log/yumaster.FileService"
  },
  "Server": {
    "ReverseProxy": true
  },
  "Db": {
    //1 - MySQL | 2 - MSSQL
    "DbType": 2,
    "MasterConnectionString": "server=127.0.0.1;user id=sa;password=123456;database=fileservice; pooling=true;",
    "FileTableCount": 64
  },
  "General": {
    "RootPath": "/data/yumaster.FileService/files",
    "AppSecret": "{F43D9A20-69DE-45D6-E298-7DACB57B9C10}", //"xxxxxxxxxxxxxxx",
    "QueryFileName": false
  },
  "ImageConverter": {
    "ConverterPath": "convert",
